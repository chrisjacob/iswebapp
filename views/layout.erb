<script type="text/javascript">
	// anything that needs to happen early on (e.g. document.write)
	
</script>

<style type="text/css">
	/* any overriding styles */
	
	/* Nudge down to get to 14px equivalent for these form elements (normally 108% for 14px) */ 
	select, input, button, textarea { font:106% arial,helvetica,clean,sans-serif; }
	
	/* Hack to target Webkit - http://themechanism.com/blog/2008/01/08/safari-css-hack-redux/ */
	/* Force Aqua style to render selects at correct font-size */
	@media screen and (-webkit-min-device-pixel-ratio:0) {
		select, input, button { font-size:122%; } /*renders 16px */
	}
	
	/* >> The Magnificent CLEARFIX << */
	.clearfix:after { content: "."; display: block; height: 0; clear: both; visibility: hidden; }
	.clearfix { display: inline-block; }
	* html .clearfix { height: 1%; } /* Hides from IE-mac \*/
	.clearfix { display: block; }
	
	/* image replace */
	.ir { display:block; text-indent:-9999px; overflow:hidden; background-repeat:none; }
	
	body {
		background: #fff;
		color: #000;
	}
	
	p.white {
		background: #000;
		color: #FFF;
	}

	#rocketbar { width: 100%; padding-bottom: 25px;
		background-color: transparent;
		background-image: -moz-linear-gradient(top, rgba(0, 0, 0, 1), rgba(0, 0, 0, 0));  /* FF3.6 */
		background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, rgba(0, 0, 0, 1)), color-stop(1, rgba(0, 0, 0, 0))); /* Saf4+, Chrome */
	}
	#rocketbar.floater { position: fixed; top: 0px; z-index: 999; } 
		#primary, #secondary{ width: 960px; margin: 0px auto; background: transparent; }
		#primary { height: 40px; overflow: hidden; position: relative; }
			#primary_inner, #secondary_inner { border-style: solid; border-color: #999; }
			#primary_inner { width: 898px; height:40px; position: absolute; left: 30px; z-index:10; padding-top: 8px; background-color: #EEE; border-width: 0px 1px 0px 1px;	
				   -moz-box-shadow: inset 1px 1px 0px #FFF, 0px -12px 5px 0px rgba(0, 0 , 0, 0.5); /* FF3.5+ */
				-webkit-box-shadow: inset 1px 1px 0px #FFF, 0px -12px 5px 0px rgba(0, 0 , 0, 0.5); /* Saf3.0+, Chrome */
				        box-shadow: inset 1px 1px 0px #FFF, 0px -12px 5px 0px rgba(0, 0 , 0, 0.5); /* Opera 10.5, IE 9.0 */
			}
				#navigation {}
					#navigation ul { list-style-type: none; height: 32px; line-height: 32px; }
					#navigation ul li { display: block; float: left; padding: 0px 10px 0px 10px; }
					#navigation ul li a { text-decoration:none; }
				#controlls {}
			#tab_left, #tab_right { width: 34px; height:29px; position: absolute; z-index:9; background-color: #DDD; border-style: solid; border-color: #999; 
				   -moz-transition: left 0.25s ease-out;  /* FF3.7+ */
				     -o-transition: left 0.25s ease-out;  /* Opera 10.5 */
				-webkit-transition: left 0.25s ease-out;  /* Saf3.2+, Chrome */
				
				   -moz-box-shadow: 0px 5px 5px -3px rgba(0, 0 , 0, 0.5), inset 1px 1px 0px #FFF; /* FF3.5+ */
				-webkit-box-shadow: 0px 5px 5px -3px rgba(0, 0 , 0, 0.5), inset 1px 1px 0px #FFF; /* Saf3.0+, Chrome */
				        box-shadow: 0px 5px 5px -3px rgba(0, 0 , 0, 0.5), inset 1px 1px 0px #FFF; /* Opera 10.5, IE 9.0 */
			}
			#tab_left { left: 5px; border-width: 0px 0px 1px 1px;
				                -moz-border-radius: 0px 0px 0px 5px; /* FF1+ */
				    -webkit-border-top-left-radius: 0px;  /* Saf3-4 */
				   -webkit-border-top-right-radius: 0px;
				-webkit-border-bottom-right-radius: 0px;
				 -webkit-border-bottom-left-radius: 5px;
				                     border-radius: 0px 0px 0px 5px; /* Opera 10.5, IE 9, Saf5, Chrome */
			}
			#tab_right { left: 920px; border-width: 0px 1px 1px 0px;
				                -moz-border-radius: 0px 0px 5px 0px; /* FF1+ */
				    -webkit-border-top-left-radius: 0px;  /* Saf3-4 */
				   -webkit-border-top-right-radius: 0px;
				-webkit-border-bottom-right-radius: 5px;
				 -webkit-border-bottom-left-radius: 0px;
				                     border-radius: 0px 0px 5px 0px; /* Opera 10.5, IE 9, Saf5, Chrome */
			}
			#tab_left:hover, #tab_left:active, #tab_left:focus { left: 0px; }
			#tab_right:hover, #tab_right:active, #tab_right:focus { left: 925px; }
		
		#secondary {}
			#secondary_inner { width: 898px; margin: 0 auto; padding-bottom: 8px; background-color: #EEE; border-width: 0px 1px 1px 1px;
				                -moz-border-radius: 0px 0px 5px 5px; /* FF1+ */
				    -webkit-border-top-left-radius: 0px;  /* Saf3-4 */
				   -webkit-border-top-right-radius: 0px;
				-webkit-border-bottom-right-radius: 5px;
				 -webkit-border-bottom-left-radius: 5px;
				                     border-radius: 0px 0px 5px 5px; /* Opera 10.5, IE 9, Saf5, Chrome */
			
				   -moz-box-shadow: 0px 5px 5px -3px rgba(0, 0 , 0, 0.5), inset 1px 0px 0px #FFF; /* FF3.5+ */
				-webkit-box-shadow: 0px 5px 5px -3px rgba(0, 0 , 0, 0.5), inset 1px 0px 0px #FFF; /* Saf3.0+, Chrome */
				        box-shadow: 0px 5px 5px -3px rgba(0, 0 , 0, 0.5), inset 1px 0px 0px #FFF; /* Opera 10.5, IE 9.0 */
			}
				#primary_extras, #flash_messages, #options { width: 100%; padding-top: 8px; display:none; 
					   -moz-box-shadow: inset 1px 0px 0px #FFF; /* FF3.5+ */
					-webkit-box-shadow: inset 1px 0px 0px #FFF; /* Saf3.0+, Chrome */
					        box-shadow: inset 1px 0px 0px #FFF; /* Opera 10.5, IE 9.0 */
				}
				#primary_extras { background-color: #F55; }
				#flash_messages { background-color: #0FF; }
				#options { background-color: #FF0; }
</style>

